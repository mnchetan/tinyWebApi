{
  "AllowedHosts": "*",
  "AllowedCorsHosts": "http://*:*;http://localhost:4200",
  "ConfigurationDirectoryPath": "",
  "environment": "local",
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Trace",
      "Microsoft.Hosting.Lifetime": "Trace"
    },
    "options": {
      "file": "Log_$|Date[dd_MMM_yyyy]|$.log",
      "size": 1073741824,
      "path":  ".\\logs"
    }
  }
}
